        "heading": "#222222",
        "icon": "#8E6DA3",
        "separator": "#EBD0E0",
        "placeholder": "#9CA3AF",
        "hover": "#FFF0F5",
        "accentForeground": "#ffffff",
        "backdrop": "#E1AFD166"
