        "background": "#FFF0F4",
        "hover": "#FFE4EF"
      },
      "secondary": {
        "background": "#FFE4EF"
      }
    },
    "navigationMenu": {
      "primary": {
        "background": "#FFF0F4",
        "hover": "#FFE4EF"
      }
    },
    "contextMenu": {
      "primary": {
        "background": "#FFF8FA"
      }
    },
    "sheet": {
      "selected": {
        "paragraph": "#388E3C"
      }
    }
