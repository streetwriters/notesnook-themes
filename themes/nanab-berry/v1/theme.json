      "success": {
        "accent": "#388E3C",
        "paragraph": "#2E7D32",
        "background": "#F1F8E9",
        "border": "#A5D6A7",
        "heading": "#388E3C",
        "icon": "#388E3C",
        "separator": "#c8e6c9",
