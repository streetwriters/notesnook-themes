{
  "$schema": "https://raw.githubusercontent.com/streetwriters/notesnook-themes/main/schemas/v1.schema.json",
  "name": "Nanab Berry",
  "id": "nanab-berry",
  "version": 1.2,
  "license": "GPL-3.0-or-later",
  "homepage": "https://notesnook.com",
  "description": "A refined pastel theme inspired by the Nanab Berry with improved readability and a pinker editor.",
  "colorScheme": "light",
  "compatibilityVersion": 1,
  "authors": [
    {
      "name": "riiku",
      "email": "ricardo.ars@pm.me",
      "url": "https://tech.lgbt/@riiku"
    }
  ],
  "scopes": {
    "base": {
