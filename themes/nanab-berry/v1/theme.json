        "hover": "#f0f0f0",
        "accentForeground": "#ffffff",
        "backdrop": "#00000033"
      },
      "selected": {
        "accent": "#E1AFD1",
        "paragraph": "#2d2d2d",
        "background": "#FFE9F0",
        "border": "#C48CB3",
        "heading": "#2d2d2d",
        "icon": "#C48CB3",
        "separator": "#f0d4e6",
        "placeholder": "#a8a8a8",
        "hover": "#ffd9e6",
        "accentForeground": "#ffffff",
        "backdrop": "#E1AFD166"
