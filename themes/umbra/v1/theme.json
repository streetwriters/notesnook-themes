      "primary": {
        "background": "#0F0F0F",
        "hover": "#141414"
      },
      "selected": {
        "background": "#1A1A1A",
        "hover": "#242424"
      }
    },
    "sheet": {
      "selected": {
        "paragraph": "#7C7C7C"
