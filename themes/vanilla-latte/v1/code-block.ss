/**  * Vanilla Latte - Code Block Theme  * A warm, coffee-inspired syntax highlighting theme for light mode  */  code[class*="language-"], pre[class*="language-"] {   color: #4A3428;   background: none;   font-family: Consolas, "Liberation Mono", Menlo, Courier, monospace;   text-align: left;   white-space: pre;   word-spacing: normal;   word-break: normal;   word-wrap: normal;   line-height: 1.5;   -moz-tab-size: 4;   -o-tab-size: 4;   tab-size: 4;   -webkit-hyphens: none;   -moz-hyphens: none;   -ms-hyphens: none;   hyphens: none; }  /* Code blocks */ pre[class*="language-"] {   padding: 1em;   margin: 0.5em 0;   overflow: auto;   background: #F9F3EB;   border: 1px solid #E8D5C4;   border-radius: 6px; }  /* Inline code */ :not(pre) > code[class*="language-"] {   padding: 0.1em 0.3em;   border-radius: 0.3em;   white-space: normal;   background: #F5EDE3;   border: 1px solid #E8D5C4; }  /* Comments */ .token.comment, .token.prolog, .token.doctype, .token.cdata {   color: #8B7264;   font-style: italic; }  /* Punctuation */ .token.punctuation {   color: #6B5C52; }  /* Namespace */ .token.namespace {   opacity: 0.7; }  /* Properties and attributes */ .token.property, .token.tag, .token.constant, .token.symbol, .token.deleted {   color: #C85A54; }  /* Boolean, number */ .token.boolean, .token.number {   color: #B87A3F; }  /* Selectors, strings, chars, built-ins, inserted */ .token.selector, .token.attr-name, .token.string, .token.char, .token.builtin, .token.inserted {   color: #7A9B6F; }  /* Operators, entities, URLs */ .token.operator, .token.entity, .token.url, .language-css .token.string, .style .token.string, .token.variable {   color: #B8885F; }  /* Keywords, important */ .token.atrule, .token.attr-value, .token.keyword {   color: #9B6B4F;   font-weight: 600; }  /* Functions */ .token.function, .token.class-name {   color: #8A6F3F; }  /* Regex, important */ .token.regex, .token.important {   color: #C87A3F; }  .token.important, .token.bold {   font-weight: bold; }  .token.italic {   font-style: italic; }  .token.entity {   cursor: help; }  /* Line highlighting */ .line-highlight {   background: #F0E6DC;   border-left: 3px solid #B8885F; }  /* Selection */ pre[class*="language-"]::selection, pre[class*="language-"] ::selection, code[class*="language-"]::selection, code[class*="language-"] ::selection {   background: #E8D5C4; }  /* YAML specific */ .language-yaml .token.atrule {   color: #9B6B4F; }  /* Bash specific */ .language-bash .token.function {   color: #8A6F3F; }  /* JSON specific */ .language-json .token.property {   color: #B8885F; }  /* Markdown specific */ .language-markdown .token.title {   color: #3D2817;   font-weight: bold; }  .language-markdown .token.code {   color: #7A9B6F; }
